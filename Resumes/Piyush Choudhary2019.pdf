                else {

                    $('#ContentPlaceHolder1_trExpc1').css("display", "block")
                    $('#ContentPlaceHolder1_trExpc2').css("display", "block")
                    $('#ContentPlaceHolder1_trComp1').css("display", "block")
                    $('#ContentPlaceHolder1_trComp2').css("display", "block")

                    $('#ContentPlaceHolder1_trExp').show();
                    $('#ContentPlaceHolder1_trComp').show();
                   
                }

