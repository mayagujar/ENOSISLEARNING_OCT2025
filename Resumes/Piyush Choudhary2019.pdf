                    $('#ContentPlaceHolder1_trComp1').css("display", "none")
                    $('#ContentPlaceHolder1_trComp2').css("display", "none")
                  
                    $('#ContentPlaceHolder1_drpExpYears').removeAttr('required');
                    $('#ContentPlaceHolder1_txtCompanyName').removeAttr('required');

